    <skill id="9206" name="Fast Moving" max-level="2" action="A2">
        <!--
            level 1: Movement Speed +95 for 30 seconds.
            level 2: Movement Speed +95 for 35 seconds.
         -->
        <icon initial="icon.skill9206"/>
        <attributes property="PHYSIC" magic-critical-rate="5.0" without-action="true">
            <magic-level initial="1"/>
            <reuse initial="45000"/>
            <effect-point initial="1"/>
            <effect-range initial="-1"/>
        </attributes>
        <target/>
        <abnormal type="SPECIAL_RIDE">
            <level initial="1"/>
            <time initial="30"/>
            <chance initial="-1"/>
        </abnormal>
        <effects>
            <speed>
                <power initial="95.0"/>
            </speed>
        </effects>
    </skill>
