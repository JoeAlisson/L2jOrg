	<skill id="59723" name="VIP Level 8">
        <!-- The right to VIP 8th level chest. XP/SP gain +30%%, item drop rate +250%%. Experience loss and chance to lose items upon death is decreased by 20%%. PvE/PvP damage is increased, STR/DEX/INT/WIT/MEN +2. World chat on any level, number of usage attempts +20. XP/SP gain +20%% during fishing. -->
        <icon initial="BranchIcon.Icon.g_skill_vip8"/>
        <attributes property="MAGIC">
        </attributes>
        <target/>
        <effects>
            <base-stat type="STR">
                <power initial="2.0"/>
            </base-stat>
			<base-stat type="DEX">
                <power initial="2.0"/>
            </base-stat>
			<base-stat type="INT">
                <power initial="2.0"/>
            </base-stat>
			<base-stat type="WIT">
                <power initial="2.0"/>
            </base-stat>
			<base-stat type="MEN">
                <power initial="2.0"/>
            </base-stat>
			<base-stat type="CON">
                <power initial="2.0"/>
            </base-stat>
            <stat-modify mode="PER" stat="BONUS_EXP">
                <power initial="35.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="BONUS_SP">
                <power initial="35.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="BONUS_DROP_RATE">
                <power initial="300.0"/>
            </stat-modify>
            <reduce-drop-penalty>
                <experience initial="-30.0"/>
                <death-penalty initial="-30.0"/>
            </reduce-drop-penalty>
            <stat-modify stat="WORLD_CHAT_POINTS">
                <power initial="50.0"/>
            </stat-modify>
			<stat-modify mode="PER" stat="PVE_MAGICAL_SKILL_DAMAGE">
                <power initial="15.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PVE_PHYSICAL_ATTACK_DAMAGE">
                <power initial="15.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PVE_PHYSICAL_SKILL_DAMAGE">
                <power initial="15.0"/>
            </stat-modify>
			 <stat-modify mode="PER" stat="PVP_MAGICAL_SKILL_DAMAGE">
                <power initial="12.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PVP_PHYSICAL_ATTACK_DAMAGE">
                <power initial="12.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="PVP_PHYSICAL_SKILL_DAMAGE">
                <power initial="12.0"/>
            </stat-modify>
			 <stat-modify stat="PHYSICAL_DEFENCE" stat-add="PHYSICAL_DEFENCE">
                <power initial="50.0"/>
            </stat-modify>
			 <stat-modify stat="MAGICAL_DEFENCE" stat-add="MAGICAL_DEFENCE">
                <power initial="50.0"/>
            </stat-modify>
            <stat-modify mode="PER" stat="FISHING_EXP_SP_BONUS">
                <power initial="20.0"/>
            </stat-modify>
			<vital-stat-modify stat="MAX_HP">
                <power initial="100"/>
            </vital-stat-modify>
			<vital-stat-modify stat="MAX_MP">
                <power initial="100"/>
            </vital-stat-modify>
        </effects>
    </skill>
	
